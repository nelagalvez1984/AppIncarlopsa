<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="incarlopsa.com.appincarlopsa">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono_incarlopsa"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icono_incarlopsa"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VGeneral">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".VLogin">
        </activity>
        <activity
            android:name=".TESTSettingsActivity"
            android:label="TEST Settings"
            android:parentActivityName=".VGeneral">
        </activity>
        <activity android:name=".VCabeceraChat">
        </activity>
        <activity android:name=".VChat">
        </activity>
        <activity android:name=".VCabeceraPublicacion">
        </activity>
        <activity android:name=".VPublicacion">
        </activity>
    </application>

</manifest>